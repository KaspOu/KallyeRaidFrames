									</OnEnter>
								</Scripts>
							</Button>
							<CheckButton name="KRFOptionsFrame_LabelRaid" inherits="KRFHeadingWidget">
								<Anchors>
									<Anchor point="TOP" relativeTo="$parent"><Offset x="0" y="0"/></Anchor>
