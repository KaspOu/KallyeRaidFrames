		<Scripts>
			<OnLoad>
				RefreshKRFOptions();
				self.name = KRF_TITLE;
				self.okay = SaveKRFOptions;
				self.refresh = RefreshKRFOptions;
				InterfaceOptions_AddCategory(self);				
				if (KRFOptionsScrollFrame ~= nil) then
					local BACKDROP_TOOLTIP = {
						bgFile = "Interface\\DialogFrame\\UI-DialogBox-Background",
						edgeFile = "Interface\\FriendsFrame\\UI-Toast-Border",
						tile = true,
						tileSize = 8,
						edge = true,
						edgeSize = 8,
						insets = {left = 2, right = 2, top = 2, bottom = 2},
					};

					if (BackdropTemplateMixin) then Mixin(KRFOptionsScrollFrame, BackdropTemplateMixin) end
					KRFOptionsScrollFrame:SetBackdrop(BACKDROP_TOOLTIP)
				end
				if KRFOptionsFrame ~= nil and KRFOptionsFrame.HandleVis ~= nil then
					KRFOptionsFrame:Hide();
				end
			</OnLoad>
		</Scripts>

